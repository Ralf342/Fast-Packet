<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="tipos">

    <select id="rolEmpleado" resultType="pojo.RolEmpleado">
        SELECT idRol,tipo
        FROM rol
    </select>
    
    <select id="tipoUnidad" resultType="pojo.TipoUnidad">
        SELECT idTipoUnidad,tipo
        FROM tipoUnidad
    </select>

</mapper>
